{
  "name": "json-to-pydantic",
  "displayName": "JSON to Pydantic",
  "description": "Generate Pydantic models automatically from JSON data.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-to-pydantic.generatePydanticCode",
        "title": "Generate Pydantic Code"
      }
    ],
    "configuration": {
      "title": "JSON to Pydantic",
      "properties": {
        "json-to-pydantic.defaultRootClassName": {
          "type": "string",
          "default": "Model",
          "description": "Sets the default name of the root class"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "sinon": "^21.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "json-to-pydantic-code-generator": "^1.0.3"
  }
}

